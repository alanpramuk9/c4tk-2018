<div class="results" style="text-align:center"><h2>Results: Moses</h2></div>
<div class="container">
  <div class="row align-items-start">
    <!--<div class="heading">-->
    <div class="col-5">
      <div class="middle">
        <div id="name"><h3>Characters</h3></div>
        <div id="filter"><%= image_tag("icons/filter.svg")%></div>
      </div>
    </div>
    <div class="col-6 align-self-center">
      <div id="tags">
        <% %w() %>
        <%  %>
        <button type="button" class="btn btn-primary">Verse</button>
        <button type="button" class="btn btn-primary">Passage</button>
        <button type="button" class="btn btn-primary">Song</button>
        <button type="button" class="btn btn-primary">Quote</button>
        <button type="button" class="btn btn-primary">Pictures</button>
        <button type="button" class="btn btn-primary">Video</button>
        <button type="button" class="btn btn-primary">Audio</button>
        <button type="button" class="btn btn-primary">Story</button>
        <button type="button" class="btn btn-primary">Jokes</button>
      </div>
    </div>
    <!--</div>-->
  </div>
</div>


<div class="main">
  <div class="left" id="left">
    <% results.each do |card| %>
      <div class="result">
        <div class="flexible">
          <%= image_tag("icons/#{card.type_identifier}.svg", class: "filetype")%>
          <div class="content">
            <p><%= card.quote %></p>
            <p><%= card.url %></p>
          </div>
          <div class="rightcontent">
            <div><%= image_tag("icons/check_yes.svg")%></div>
            <div><%= image_tag("icons/more_info.svg")%></div>

          </div>
        </div>
      </div>
    <% end %>
  </div>

  <%# render partial: "search/image_board" %>
</div>